INSTANCE 1
[WINDOW]
Window  WINDOW('Propiedades de Campos'),AT(,,245,183),FONT('MS Sans Serif',8,,FONT:regular), |
          RESIZE,CENTER,GRAY,SYSTEM
          OPTION,AT(8,4,110,17),USE(loc:quienes),BOXED,#ORDINAL(1)
            RADIO('Modificar'),AT(69,9,42),USE(?OPTION1:RADIO1),VALUE('T'),#ORDINAL(2)
            RADIO('por Defecto'),AT(11,9,58,10),USE(?OPTION1:RADIO1:3),VALUE('D'),#ORDINAL(3)
          END
          STRING(@s100),AT(16,32,163),USE(PCT:NOMBRE_FEQ),FONT('Microsoft Sans Serif',,,FONT:bold), |
            #ORDINAL(4)
          GROUP,AT(11,46,177,64),USE(?GROUPetiqueta),TRN,#ORDINAL(5)
            PROMPT('Etiqueta:'),AT(16,56),USE(?Prompt:Text),#ORDINAL(6)
            ENTRY(@s255),AT(71,55,107,10),USE(PCT:TEXT),#ORDINAL(7)
            CHECK('Deshabilitar (Grisar)'),AT(70,78),USE(PCT:DISABLE),TRN,#ORDINAL(8)
            CHECK('Ocultar'),AT(70,98,41,10),USE(PCT:HIDE),#ORDINAL(9)
          END
          GROUP,AT(11,113,177,68),USE(?GROUPentry),TRN,#ORDINAL(10)
            PROMPT('Valor inicial:'),AT(16,122),USE(?PCT:PRIME:Prompt),#ORDINAL(11)
            ENTRY(@s255),AT(71,122,107,10),USE(PCT:PRIME),#ORDINAL(12)
            CHECK('Requerido (Obligatorio)'),AT(70,143,109,10),USE(PCT:REQUIRED),#ORDINAL(13)
            CHECK('Solo Lectura'),AT(70,163,109,10),USE(PCT:READONLY),#ORDINAL(14)
          END
          BUTTON('&Grabar'),AT(192,143,46,14),USE(?Ok:2),LEFT,#ORDINAL(15)
