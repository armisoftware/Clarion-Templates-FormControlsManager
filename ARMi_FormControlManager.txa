LlenoDesdeDefecto   ROUTINE
    setcursor(cursor:wait)
    clear(PCT:record)
    PCT:USU_ID = -1
    PCT:PROCEDIMIENTO = loc:Prodecimiento
    set(PCT:xUSU_PRO_PCT,PCT:xUSU_PRO_PCT)
    LOOP
        NEXT(PAR_CTRLS)
        IF ERROR() |
            or PCT:USU_ID <> -1 |
            or PCT:PROCEDIMIENTO <> loc:Prodecimiento
                BREAK
        END
        clear(PCT1:Record)
        PCT1:USU_ID = -1
        PCT1:PROCEDIMIENTO = loc:Prodecimiento
        PCT1:FEQ = PCT:FEQ
        get(PAR_CTRLS_1,PCT1:xUSU_PRO_FEQ_PCT)
        IF ERROR()
            PCT1:RECORD :=: PCT:RECORD
            PCT1:USU_ID  = loc:Usu_Id
            Access:par_ctrls_1.Insert()
        end
    end
    setcursor()
[END]
