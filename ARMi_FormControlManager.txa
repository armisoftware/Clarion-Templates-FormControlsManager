NAME ABC CloseCurrentWindow
[INSTANCE]
INSTANCE 7
[WINDOW]
Window  WINDOW('Propiedades de Campos'),AT(,,389,218),FONT('MS Sans Serif',8,,FONT:regular), |
          RESIZE,CENTER,GRAY,SYSTEM
          BUTTON('&Cerrar'),AT(338,202,46,14),USE(?CancelButton),LEFT,STD(STD:Close),#ORDINAL(1)
          LIST,AT(4,36,380,162),USE(?List),RIGHT(1),VSCROLL,FORMAT('23R(2)|~FEQ~L(2)@n-' & |
            '14@80L(2)|M*~NOMBRE FEQ~@s20@20L(2)|P~OCULTAR~@n1B@Q''ESCONDER''20L(2)|P~G' & |
            'RISAR~@n1B@Q''GRISAR''20L(2)|P~REQUERIDO~@n1B@Q''REQUERIDO''20L(2)|P~SOLO ' & |
            'LECTURA~@n1B@Q''SOLO LECTURA''66L(2)|MP~TEXT/FORMATO~@s250@Q''TEXTO O FORM' & |
            'ATO''1000L(2)|MP~VALOR INICIAL~@s250@Q''VALOR ACTUAL'''),FROM(Queue:Browse),IMM, |
            VCR,#FIELDS(PCT:FEQ,PCT:NOMBRE_FEQ,PCT:HIDE,PCT:DISABLE,PCT:REQUIRED,PCT:READONLY, |
            PCT:TEXT,PCT:PRIME),#ORIG(?List),#SEQ(6),#ORDINAL(2)
          BUTTON('&Insert'),AT(136,160,42,12),USE(?Insert),HIDE,#ORIG(?Insert),#SEQ(8),#ORDINAL(3)
          BUTTON('&Change'),AT(178,160,42,12),USE(?Change),HIDE,#ORIG(?Change),#SEQ(8),#ORDINAL(4)
          BUTTON('&Delete'),AT(220,160,42,12),USE(?Delete),HIDE,#ORIG(?Delete),#SEQ(8),#ORDINAL(5)
          OPTION,AT(8,0,127,17),USE(loc:quienes),BOXED,TRN,#ORDINAL(6)
            RADIO('Modificados'),AT(70,5),USE(?OPTION1:RADIO1),TRN,VALUE('T'),#ORDINAL(7)
            RADIO('por Defecto'),AT(12,5,58,10),USE(?OPTION1:RADIO1:3),TRN,VALUE('D'),#ORDINAL(8)
          END
          PROMPT('Buscar:'),AT(9,22),USE(?loc:Buscar:Prompt),#ORDINAL(9)
          ENTRY(@s50),AT(38,22,207,10),USE(loc:Buscar),UPR,#ORDINAL(10)
          PROMPT('Recuerde: Solo se actualizan los valores por defecto si esta en modo ' & |
            'Agregar registro.<0DH,0AH>Los controles en Azul son ingreso de datos, son ' & |
            'los nicos que pueden tener valor inicial.'),AT(3,201,291,22),USE(?loc:Buscar:Prompt:2), |
            #ORDINAL(11)
          PROMPT('Ancho Ventana:'),AT(150,6),USE(?loc:AnchoVentana:Prompt),#ORDINAL(12)
          ENTRY(@n-14),AT(207,6,37,10),USE(loc:AnchoVentana),RIGHT(1),#ORDINAL(13)
          PROMPT('Alto Ventana:'),AT(252,6),USE(?loc:AltoVentana:Prompt),#ORDINAL(14)
          ENTRY(@n-14),AT(302,6,37,10),USE(loc:AltoVentana),RIGHT(1),#ORDINAL(15)
        END

