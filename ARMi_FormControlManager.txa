EMBED %WindowManagerMethodCodeSection
[INSTANCES]
WHEN 'Init'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 7750
PROPERTY:END
loc:quienes='T' ! arranco en Todos
[SOURCE]
PROPERTY:BEGIN
PRIORITY 9003
PROPERTY:END
      if loc:quienes='D'
          loc:Usu_Id = -1
          Do Leer
      elsif loc:quienes='T'
          loc:Usu_Id = -1
          DO Leer
          loc:Usu_Id = 0
          DO Leer
      end
[SOURCE]
PROPERTY:BEGIN
PRIORITY 9003
PROPERTY:END
if pType=create:Entry or pType=create:Text or pType=create:list
    enable(?GROUPentry)
    ?Prompt:Text{PROP:Text} = 'Formato:'
ELSE
    DISABLE(?GROUPentry)
    ?Prompt:Text{PROP:Text} = 'Etiqueta:'
END
if pType=CREATE:list
    ?PCT:PRIME:Prompt{PROP:Text} = 'Iniciar en Nr:'
    ?PCT:PRIME{PROP:Text}        = '@N3'
END


[END]
[END]
[END]
EMBED %ControlEventHandling
[INSTANCES]
WHEN '?loc:quienes'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
if loc:quienes='D'
    disable(?GROUPentry)
    disable(?GROUPetiqueta)
    loc:Usu_Id = -1
    do Leer
elsif loc:quienes='T'
    enable(?GROUPentry)
    enable(?GROUPetiqueta)
    loc:Usu_Id = 0
    DO Leer
end





[END]
[END]
WHEN '?Close'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[GROUP]
PRIORITY 5000
INSTANCE 7
[END]
[END]
WHEN '?Ok:2'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
Access:PAR_CTRLS.Update()
[GROUP]
PRIORITY 4000
INSTANCE 1
[END]
[END]
[END]
EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
Leer                ROUTINE
    clear(PCT:Record)
    PCT:USU_ID   = loc:USU_ID
    PCT:PROCEDIMIENTO = pProcedure
    PCT:NOMBRE_FEQ    = pNombreFeq
    if Access:par_ctrls.fetch(PCT:xUSU_PRO_PCT)
         clear(PCT:Record)
         PCT:USU_ID   = loc:USU_ID
         PCT:PROCEDIMIENTO = pProcedure
         PCT:FEQ      = pFeq
         PCT:NOMBRE_FEQ   = pNombreFeq
         PCT:COLUMNA  = 0
         PCT:TEXT     = pText
         PCT:DISABLE  = pDisable
         PCT:HIDE     = pHide
         if inlist(pType,create:entry,create:text)
           PCT:PRIME    = pUse
           PCT:REQUIRED = pReq
           PCT:READONLY = pREADONLY
           PCT:COLUMNA  = 0  !SON EDITABLES
         else
           PCT:REQUIRED = 0
           PCT:READONLY = 0
           PCT:PRIME    = ''
           PCT:COLUMNA  = 1  !NO SON EDITABLES
         end
        Access:par_ctrls.Insert()
    END
    display
[END]
[END]
